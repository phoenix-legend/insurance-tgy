<h1>这是预约界面</h1>
<%= form_tag(create_appointment_order_system_products_path,method: 'post') do %>
    <div class="row">
      <label>车牌号</label>
      <%= text_field_tag :car_number,@car_number %>
    </div>
    <div class="row">
      <label>手机号</label>
      <%= text_field_tag :phone,@phone %>
    </div>
    <div class="row">
      <%= submit_tag "免费预约"%>
    </div>
<% end %>